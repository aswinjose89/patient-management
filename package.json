{
  "name": "patient-management",
  "version": "1.3.1",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=\"--max-old-space-size=3072\" && ng serve",
    "hmr": "ng serve --hmr",
    "build": "ng build",
    "test": "export NODE_OPTIONS=\"--max-old-space-size=3072\" && ng test",
    "test-ci": "ng test --watch=false --browsers ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "goal-data": "curl https://hapi-test.cardihab.com/baseDstu3 --data-binary \"@src/assets/test/bootstrap/goals.json\" -H \"Content-Type: application/fhir+json\"",
    "bootstrap-data": "curl https://hapi-test.cardihab.com/baseDstu3 --data-binary \"@src/assets/test/bootstrap/bootstrap.json\" -H \"Content-Type: application/fhir+json\"",
    "demo-data": "curl https://hapi-test.cardihab.com/baseDstu3 --data-binary \"@src/assets/test/demo.json\" -H \"Content-Type: application/fhir+json\"",
    "sam-jones-data": "curl https://hapi-test.cardihab.com/baseDstu3 --data-binary \"@src/assets/test/sam_jones/sam_jones.json\" -H \"Content-Type: application/fhir+json\"",
    "version": "echo $npm_package_version",
    "upload-sentry-sourcemaps": "sentry-cli --auth-token $SENTRY_AUTH_TOKEN releases -o cardihab-pty-ltd -p pam files $(npm run version --silent) upload-sourcemaps --url-prefix / dist --rewrite"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.15",
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "@angular/material": "^11.1.1",
    "@angular/material-moment-adapter": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@angular/router": "^11.1.1",
    "@fullcalendar/angular": "^5.5.0",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/list": "^5.5.0",
    "@sentry/browser": "^6.0.2",
    "@swimlane/ngx-charts": "^17.0.0",
    "@swimlane/ngx-datatable": "^19.0.0",
    "amazon-chime-sdk-js": "^2.4.0",
    "angularx-qrcode": "^11.0.0",
    "aws-amplify": "^3.3.14",
    "aws-amplify-angular": "^5.0.43",
    "aws-sdk": "^2.832.0",
    "d3": "^6.5.0",
    "file-saver": "^2.0.5",
    "libphonenumber-js": "^1.9.11",
    "moment": "^2.29.1",
    "ngx-mat-intl-tel-input": "^4.0.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.16.9",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.1",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/language-service": "^11.1.1",
    "@angularclass/hmr": "^3.0.0",
    "@sentry/cli": "^1.62.0",
    "@types/fhir": "3.0.2",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "4.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.1"
  }
